{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": { "dependsOn": ["^build"], "outputs": ["dist/**"] },
    "dev": { "persistent": true, "cache": false },
    "start": { "dependsOn": ["build"], "persistent": true },
    "migrate": { "cache": false },
    "test": { "dependsOn": ["^build"] }
  }
}
